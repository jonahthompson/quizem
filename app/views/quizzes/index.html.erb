<div class="container">
	<div class="card">
		<div class="card-header">
			<h2>Surveys</h2>
		</div>
		<div class="card-body">
			<ul>
			<% @quizzes.each do |quiz| %>
			 	<li>Name:<%= quiz.name %> <%= link_to "Take Survey", quiz_path(quiz) %></br> Subject:<%= quiz.subject %></li>
			<% end %>
			</ul>
		</div>
	</div>
</div>